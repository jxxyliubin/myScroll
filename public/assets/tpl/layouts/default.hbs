    <title>{{title}}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="keywords" content="{{keywords}}">
    <meta name="description" content="{{description}}">
    <link rel="shortcut icon" type="image/x-icon" href="http://res.yy.com/common/images/favicon.ico"/>
    <link rel="stylesheet" href="{{stylesheet}}?ver={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}">
